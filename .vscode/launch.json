{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Aktuelle Datei",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [ // Testcase: Compile Tests
                "Tests,-In Source,-Ex Source\\SampleApp.cpp",
                "-I Source -I Source\\Modules -I Tests\\Include",
            ]
            // "args": [ // Testcase: Compile SampleApp
            //     "Source",
            //     "-I Source -I Source\\Modules",
            // ]

        },
        {
            "name": "Launch LEL Demo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/LEL.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\mingw\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // Gets sure source is compiled before debugging 
            "preLaunchTask": "Manually: build Project"
        }
    ]
}