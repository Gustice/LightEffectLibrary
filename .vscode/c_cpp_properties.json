{
    "configurations": [
        {
            "name": "MinGW",
            "intelliSenseMode": "clang-x64",
            "compilerPath": "C:\\MinGW\\bin\\gcc.exe",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Source/Modules/**",
                "${workspaceFolder}/Externals/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/Source/Modules/**",
                    "${workspaceFolder}/Externals/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cppStandard": "c++11",
            "cStandard": "c99"
        }
    ],
    "version": 4
}